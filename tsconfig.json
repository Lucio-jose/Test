{
   "compilerOptions": {
      "lib": [
         "es5",
         "es6",
         "DOM",
         "esnext.asynciterable"
      ],
      "target": "es5",
      "module": "commonjs",
      "moduleResolution": "node",
      "outDir": "./build",
      "rootDir": "src",
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "esModuleInterop":true,
      "sourceMap": true,
      "strict": false,
      "forceConsistentCasingInFileNames":true
   }
}